public class MPrismaSegitiga {

    public static void main(String[] args) {
        // Membuat objek Segitiga
        Segitiga s1 = new Segitiga(3, 4);


        // Membuat objek PrismaSegitiga
        PrismaSegitiga ps1 = new PrismaSegitiga(s1, 5);


        // Menampilkan hasil
        System.out.println("_________Hasil Perhitungan_________");
        System.out.println(("Volume Prisma Segitiga: " + ps1.hitungVolumePrisma()) + "\n");
        System.out.println("Luas Permukaan Prisma Segitiga: " + ps1.luasPermukaanPrisma());
    }
}
